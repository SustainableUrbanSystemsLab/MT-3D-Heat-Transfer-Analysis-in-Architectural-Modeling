FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/Plaster";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 8;

boundaryField
{
    maxX
    {
        type            zeroGradient;
    }
    geom
    {
        type            zeroGradient;
    }
    Plaster_to_Slab
    {
        type            calculated;
        value           uniform 0;
    }
    Plaster_to_IntairTop
    {
        type            calculated;
        value           uniform 0;
    }
    Plaster_to_ConcreteTop
    {
        type            calculated;
        value           uniform 0;
    }
}


